<h1>Create a new project</h1>

<form action="/projects" method="POST">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" id="name" name="name" >
  </div>

  <div class="form-group">
    <label for="githubLink">Github Link</label>
    <input type="text" class="form-control" id="githubLink" name="githubLink" >
  </div>

  <div class="form-group">
    <label for="deployedLink">Deployed Link</label>
    <input type="text" class="form-control" id="deployedLink" name="deployedLink" >
  </div>

  <div class="form-group">
    <label for="description">Description</label>
    <textarea type="text" class="form-control" id="description" name="description" ></textarea>
  </div>

  <div class="form-group">
    <label>Categories</label>
    <input class="form-control" type="text" name="categories" placeholder="Comma separated categories the project falls in.">
  </div>

  <input type="submit" class="btn btn-primary">
</form>
