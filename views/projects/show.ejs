<h1><%= project.name %></h1>

<p><%= project.description %></p>

<hr />

<a class="btn btn-primary" href="<%= project.githubLink %>" target="_blank">
  <i class="fa fa-github" aria-hidden="true"></i>
  Github Link
</a>

<a class="btn btn-primary" href="<%= project.deployedLink %>" target="_blank">
  <i class="fa fa-globe" aria-hidden="true"></i>
  Deployed Link
</a>

<hr />

<form action="/projects" method="PUT">
  <button type="button" id="editProject" name="editProject" class="btn btn-info">Edit this Project</button>
</form>

<form action="/projects/:id" method="POST">
  <button type="button" id="deleteProject" name="deleteProject" class="form-control btn btn-danger">Delete this Project</button>
</form>

<hr />

<a href="/">&larr; Back to Home</a>
