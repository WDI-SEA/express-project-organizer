<h1><%= project.name %></h1>

<a class="btn btn-primary" href="<%= project.githubLink %>" target="_blank">
  <i class="fa fa-github" aria-hidden="true"></i>
  Github Link
</a>

<a class="btn btn-primary" href="<%= project.deployedLink %>" target="_blank">
  <i class="fa fa-globe" aria-hidden="true"></i>
  Deployed Link
</a>

<p><%= project.description %></p>

<hr />

<strong>Categories:</strong>
<% project.categories.forEach(function(c){ %>
  ::
  <a href="/categories/<%= c.id %>"><%= c.name %></a>
<% }); %>

<br>

<a href="/projects/<%= project.id %>" class="btn btn-danger" id="delete-project">Delete Project</a>

<br>

<a href="/">&larr; Back Home</a>
