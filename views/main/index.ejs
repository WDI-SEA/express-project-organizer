<h1>Project Organizer</h1>

<h2>My Projects</h2>

<% projects.forEach(function(project) { %>
  <div class="well">
    <h2>
      <a href="/projects/<%= project.id %>"><%= project.name %></a>
      <small>by <%= project.name %></small>
    </h2>
  </div>
<% }); %>





<h2>My Categories</h2>

<% projects.forEach(function(project) { %>
  <% project.categories.forEach(function(category) { %>
  <div class="well">
    <h2>
      <a href="/categories/<%= category.id %>"><%= category.category %></a>
      <small>by <%= category.category %></small>
    </h2>
  </div>

  <% }); %>
<% }); %>
