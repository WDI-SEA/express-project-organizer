<h1 class="text-center">Categories</h1>
<% categories.forEach(c => { %>
    <div class="well">
        <div class="col s4">
            <div class="card blue-grey darken-1 center-align">
                <h2>
                    <a href="/categories/<%= c.id %>"><span class="card-title red-text"><strong><%= c.name %></strong></a></span>
                </h2>
            </div>
        </div>
    </div>
<% }) %>