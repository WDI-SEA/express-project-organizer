<h2>All Categories</h2>
<hr>

<% if (categories.length < 1) { %>
    <h4>Better get coding, I've no projects!</h4>
<% } %>

<% categories.forEach(function(category) { %>
    <div class="well">
        <h2>
        <a href="/categories/<%= category.id %>"><%= category.name %></a>
        </h2>
    </div>
<% }); %>
