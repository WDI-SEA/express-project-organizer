<h1>All Categories</h1>
<section class="main-content">
<% categories.forEach(c => {%>
    <a class="links-cat" href="/categories/<%=c.id%>">
        <h2><%=c.name%></h2>
        <% if(c.projects.length === 1) {%>
        <p>View <%= c.projects.length%> <%=c.name%> Project</p>
    <%} else { %>
        <p>View <%= c.projects.length%> <%=c.name%> Projects</p>
   <% } %>
    </a>
<%})%>
</section>