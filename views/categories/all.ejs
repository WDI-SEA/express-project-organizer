<h2> all available categories </h2>
<% if(categories && categories.length > 0){ %>
	<% categories.forEach( function(c){ %>
		<div class="well">
			<a href="/categories/<%= c.id%>"><%= c.name%></a>
			<a href="/categories/<%= c.id %>" class="btn btn-danger" id='delete-cat'>
	Permanently Delete Category and All References?
</a>
		</div>
	<% }); %>
<% } else { %>
	<h3> No categories Yet </h3>
<% } %>
