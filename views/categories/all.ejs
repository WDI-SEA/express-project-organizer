<h2 class="title">Categories</h2>
<% if(categories && categories.length >0){%>
	<% categories.forEach(function(category){%>
		<div class="well"><a href="/categories/<%=category.id%>"><h3 class="category-list"><%=category.content%></h3></a></div>
	<%})%>
<%}else{%>
	<h3 class="category-list">No categories yet!</h3>
<%}%>