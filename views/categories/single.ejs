<h1><%= categories.name %></h1>
<p><em>The below projects used <%= categories.name %>:</em></p>
<% categories.projects.forEach(function(p) { %>
  <div>
    <h2>
      <a href="/projects/<%= p.id %>"><%= p.name %></a>
    </h2>
  </div>
<% }); %>
<a href="/categories/<%= categories.id %>" class="button is-primary is-outlined is-small" id="delete-category">Delete Category</a>